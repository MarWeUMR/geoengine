    // "shutdownAction": "stopCompose",
    // "postCreateCommand": "cargo test --package geoengine-operators --lib -- processing::meteosat::xgboost::tests::test_ok --exact --nocapture",
    "workspaceFolder": "/workspaces/geoengine",
